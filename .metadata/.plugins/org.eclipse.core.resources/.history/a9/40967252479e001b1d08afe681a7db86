package com.home.test.top.ques;

public class ReverseInteger {

	public static void main(String[] args) {
		int i = 145;
		String s = "";
		int n =i;
		int r = 0;
		int rev = 0;
		
		while(n >0) {
			r = n % 10;
			n = n / 10;
			s = s + Integer.toString(r);
			rev = rev * 10 + r;
			System.out.println("last num is  last number"+r);
			System.out.println("next num is "+ n);
		}
		
		System.out.println(String.valueOf(s));
		System.out.println(rev);
	}

}
