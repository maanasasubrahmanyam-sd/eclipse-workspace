package com.home.test.top.ques;


public class PrintPatter {

//i1    * 
//i2	*  * 
//i3	*  *  * 
//i4	*  *  *  * 
//i5	*  *  *  *  * 
	//  j1 j2 j3 j4 j5
	
	public static void main(String[] args) {

		for(int i =1; i<=5 ; i++) {
			for(int j =1; j<=5; j++) {
				if(j <= i) {
					System.out.print(j);
				}
			}
			System.out.println("");
		}
	}

}
